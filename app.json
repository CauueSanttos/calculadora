{
  "expo": {
    "name": "Calculadora",
    "slug": "Calculadora",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#01911e"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "package": "com.devcaue.calculator",
      "adaptiveIcon": {
        "backgroundColor": "#01911e",
        "foregroundImage": "./assets/icon.png"
      }
    },
    "ios": {
      "supportsTablet": true
    }
  }
}
